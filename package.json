    "test": "jest"
  },
  "dependencies": {
    "fast-deep-equal": "^3.1.3"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.32.0",
    "husky": "^8.0.3",
    "jest": "^29.4.0",
    "lint-staged": "^13.1.0",
    "peggy": "^2.0.1",
    "prettier": "^2.8.3",
    "ts-jest": "^29.0.5",
    "type-fest": "^3.5.3",
    "typescript": "^4.9.4"
  },
  "lint-staged": {
    "*.{ts,md}": "prettier --list-different",
